{
	"main": "dist/_worker.js/index.js",
	"name": "apavayan",
  "compatibility_date": "2026-02-01",
  "compatibility_flags": [
    "nodejs_compat",
    "global_fetch_strictly_public"
  ],
	"routes": [
		"apavayan.com/*",
		"www.apavayan.com/*"
	],
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist"
	},
	"observability": {
    "enabled": true
  }
}